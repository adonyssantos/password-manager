{
  "name": "password-manager",
  "version": "0.0.0",
  "main": "src/main.tsx",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "emulators": "npx firebase emulators:start",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint src/ --ext .ts,.tsx, --fix",
